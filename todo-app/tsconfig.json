{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "ESNext",
	  "lib": ["DOM", "DOM.Iterable", "ESNext"],
	  "jsx": "react-jsx",
	  "moduleResolution": "node",
	  "strict": true,
	  "skipLibCheck": true,
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["src"]
  }